<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:p="http://www.springframework.org/schema/p"
       xmlns:util="http://www.springframework.org/schema/util"
       xsi:schemaLocation="
  http://www.springframework.org/schema/beans 
  http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
  http://www.springframework.org/schema/util 
  http://www.springframework.org/schema/util/spring-util-3.0.xsd">
 
     <!--here all the url value should contains the valid path for the jrxml file-->
     
    
          
          
  <bean id="studentRegistrationReport"	
          class="org.springframework.web.servlet.view.jasperreports.JasperReportsPdfView"
          p:url="classpath:jrxmlReport/registration.jrxml"/> 
          
          
  <bean id="meritList"	
          class="org.springframework.web.servlet.view.jasperreports.JasperReportsPdfView"
          p:url="classpath:jrxmlReport/meritListReport.jrxml"
          />
          
  <bean id="multipleRegistration"	
          class="org.springframework.web.servlet.view.jasperreports.JasperReportsPdfView"
          p:url="classpath:jrxmlReport/multipleRegistrationReport.jrxml"
          />
          
  <bean id="idCardReport"	
          class="org.springframework.web.servlet.view.jasperreports.JasperReportsPdfView"
          p:url="classpath:jrxmlReport/idCardReport.jrxml"
          />
          
  <bean id="horizontalRegistrationReport"	
          class="org.springframework.web.servlet.view.jasperreports.JasperReportsPdfView"
          p:url="classpath:jrxmlReport/horizontalRegistrationReport.jrxml"
          />
          
  <bean id="verticalRegSinglePDF"	
          class="org.springframework.web.servlet.view.jasperreports.JasperReportsPdfView"
          p:url="classpath:jrxmlReport/verticalRegisterReport.jrxml"
          />
          
  <bean id="addressReport"	
          class="org.springframework.web.servlet.view.jasperreports.JasperReportsPdfView"
          p:url="classpath:jrxmlReport/addressesReports.jrxml"
          />
          
  <bean id="admissionNotification"	
          class="org.springframework.web.servlet.view.jasperreports.JasperReportsPdfView"
          p:url="classpath:jrxmlReport/admissionNotificationReport.jrxml"
          />
          
  <bean id="studentRollNoList"	
          class="org.springframework.web.servlet.view.jasperreports.JasperReportsPdfView"
          p:url="classpath:jrxmlReport/studentRollNoListReport.jrxml"
          />
          
